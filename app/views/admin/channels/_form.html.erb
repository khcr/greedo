<%= form_for [:admin, @channel] do |f| %>

  <%= render 'error_messages', object: f.object %>

  <ul>
    <li>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </li>
    <li>
      <%= f.label :description %>
      <%= f.text_area :description %>
    </li>
    <li>
      <%= f.label :private %>
      <%= f.check_box :private %>
    </li>
    <li>
      <%= f.label :image %>
      <%= f.file_field :image %>
    </li>
  </ul>
  
  <ul>
    <li>
      <%= f.submit %>
    </li>
  </ul>

<% end %>